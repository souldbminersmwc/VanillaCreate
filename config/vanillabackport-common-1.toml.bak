	#Determine if dried ghasts should generate in Nether Fossils
	generateDriedGhasts = true
	#Determine the speed of happy ghasts when ridden
	# Default: 1.0
	# Range: 0.0 ~ 10.0
	happyGhastSpeedModifier = 1.0
